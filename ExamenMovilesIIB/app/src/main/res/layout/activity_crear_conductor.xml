<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CrearConductorActivity">

    <TextView
            android:text="Nombre"
            android:layout_width="125dp"
            android:layout_height="29dp"
            android:id="@+id/textView_nombre" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintVertical_bias="0.103"/>
    <TextView
            android:text="Apellido"
            android:layout_width="121dp"
            android:layout_height="33dp"
            android:id="@+id/textView_apellido" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView_nombre"
            app:layout_constraintEnd_toEndOf="@+id/textView_nombre"
            app:layout_constraintStart_toStartOf="@+id/textView_nombre" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.053"/>
    <TextView
            android:text="Fecha de Nacimiento"
            android:layout_width="151dp"
            android:layout_height="38dp"
            android:id="@+id/textView_fechaNacimiento" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView_apellido"
            app:layout_constraintStart_toStartOf="@+id/textView_apellido"
            app:layout_constraintEnd_toEndOf="@+id/textView_apellido" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.566"
            app:layout_constraintVertical_bias="0.073"/>
    <TextView
            android:text="Numero Autos"
            android:layout_width="138dp"
            android:layout_height="30dp"
            android:id="@+id/textView_numeroAutos" app:layout_constraintEnd_toEndOf="@+id/textView_fechaNacimiento"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView_fechaNacimiento" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.068"
            app:layout_constraintStart_toStartOf="@+id/textView_fechaNacimiento"
            app:layout_constraintHorizontal_bias="0.461"/>
    <Button
            android:text="GUARDAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button_guardar" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="80dp"
            android:layout_marginRight="80dp"
            android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/switch_licenciaValida" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"/>
    <Switch
            android:text="licencia Valida"
            android:layout_width="175dp"
            android:layout_height="24dp"
            android:id="@+id/switch_licenciaValida" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/textView_numeroAutos"
            app:layout_constraintHorizontal_bias="0.544"/>
    <Button
            android:text="CANCELAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button5" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/button_guardar"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/switch_licenciaValida" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.434"
            app:layout_constraintVertical_bias="0.367"/>
    <TextView
            android:text="CONDUCTOR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_conductor" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/textView_nombre"
            app:layout_constraintHorizontal_bias="0.025" app:layout_constraintVertical_bias="0.38"/>
    <Button
            android:text="iniciar"
            android:layout_width="81dp"
            android:layout_height="30dp"
            android:id="@+id/but_iniciar" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toEndOf="@+id/textView_conductor"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.23"
            app:layout_constraintVertical_bias="0.0"/>
</android.support.constraint.ConstraintLayout>